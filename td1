class(vjour)
vjour[c(2,7)]
vectbo=c(TRUE,FALSE,TRUE,FALSE,TRUE)
class(vectbo)
vectnum=c(1.2,2.8,6.5,8.9,7.8)
class(vectnum)
vectnum[-3]
